@section scripts
{
    @Scripts.Render("~/bundles/jqueryval")

    <script>
        $(document).ready(function () {
            $('#Male').select();
            $('#Male').select();
            $('#proceed').attr('disabled', 'disabled');
            $('#step1 input[type=checkbox]').on('change', function () {
                var checked = $('#step1 input[type=checkbox]:checked').length;
                var total = $('#step1 input[type=checkbox]').length;
                if (checked === total) {
                    $('#proceed').removeAttr('disabled');
                } else {
                    $('#proceed').attr('disabled', 'disabled');
                }
            });
            $('ul.timeline.timeline-horizontal .timeline-item.1 div').addClass('active');
            $('#proceed').on('click', function () {
                $('#step1').addClass('hidden');
                $('#step2').removeClass('hidden');
                $('#step2').fadeIn();
                $('ul.timeline.timeline-horizontal .timeline-item.2 div').addClass('active');
            });

            var form = $('#myform');
            form.on('submit', submitEmail);
            form.validate({ // initialize the plugin
                rules: {
                    email: {
                        required: true,
                        email: true
                    },
                    firstName: {
                        required: true
                    },
                    lastName: {
                        required: true
                    }
                }
            });

            function submitEmail(event) {
                if (!form.valid())
                    return false;
                $('#step2').addClass('hidden');
                $('#step3').removeClass('hidden');
                $('#step3').fadeIn();
                $('ul.timeline.timeline-horizontal .timeline-item.3 div').addClass('active');
                event.preventDefault();
                return false;
            }

            $('#proceed3').on('click',
                function () {
                    if (!$('#walletAddress').val() || !$('#amount').val())
                        return false;

                    var formFields = {
                        FirstName: $('#FirstName').val(),
                        LastName: $('#LastName').val(),
                        WalletAddress: $('#walletAddress').val(),
                        Email: $('#email').val(),
                        Nationality: $('#Nationality').val(),
                        Gender: $("input[name=Gender]:checked").val() === 'Male' ? 1 : 0,
                        PassportOrNIC: $('#FirstName').val(),
                        ContributionAmount: $('#amount').val(),
                        CurrencyType: $("input[name=Currency]:checked").val(),
                        Phone: $('#phone').val(),
                        State: $('#state').val(),
                    };
                    var formData = new FormData();
                    formData.append('data', JSON.stringify(formFields));
                    formData.append('file', $('#userFile')[0].files[0]);

                    $('#step3').fadeOut();
                    $('#step3').addClass('hidden');
                    $('#step4').removeClass('hidden');
                    $('#step4').fadeIn();
                    $('ul.timeline.timeline-horizontal .timeline-item.4 div').addClass('active');
                    $('#finishButton').attr('disabled', 'disabled');

                    setTimeout(function () {
                        $.ajax({
                            url: '/api/Contribute',
                            method: 'POST',
                            async: false,
                            cache: false,
                            contentType: false,
                            enctype: 'multipart/form-data',
                            processData: false,
                            data: formData,
                            success: function (response) {
                                $('#finalDialog').html('Thank you for your Exchange Request in this ITO. <br/>' +
                                    'After our KYC and AML Compliance you will receive an invitation via email to submit an Exchange Offer and send your ETH.');
                                $('#finishButton').removeAttr('disabled');
                            },
                            error: function (err) {
                                $('#finalDialog').html('Something went wrong file processing your request.');
                                $('#finishButton').removeAttr('disabled');
                            },
                            complete: function (response) {
                                //setTimeout(function () { window.location.reload(); }, 3500);
                            }
                        });
                    }, 500);
                });

            $('#finishButton').on('click',
                function () {
                    window.location.reload();
                });

            $('#userFile').on('change', function () {
                var file = $('#userFile')[0].files[0];
                $('#fileHolder').val(file.name);
            });
        });
    </script>


}
